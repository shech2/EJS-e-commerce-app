    <section>
      <div class="pro-container" id="container">

        <% for (index=0; index < 8; index++) { %>
          <div class="pro">
            <form id="prodForm" action="/product-page" method="GET">
              <button style="border: none;"><img src="<%= ProductModel[index].image %>" alt=""></button>
              <input type="hidden" name="id" value="<%= ProductModel[index].id %>">
            </form>
            <div class="des">
              <span>
                <%=ProductModel[index].brand.name %>
              </span>
              &nbsp<span>
                <%=ProductModel[index].category.name %>
              </span><br>
              <h5>
                <%= ProductModel[index].Product_name %>
              </h5>
              <div id="rating">
                <% for (rat=0; rat < ProductModel[index].rating; rat++) { %>
                  <span class="star">
                    <i class="fas fa-start"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                        fill="currentColor" class="bi bi-star-fill" viewBox="0 0 16 16">
                        <path
                          d="M3.612 15.443c-.386.198-.824-.149-.746-.592l.83-4.73L.173 6.765c-.329-.314-.158-.888.283-.95l4.898-.696L7.538.792c.197-.39.73-.39.927 0l2.184 4.327 4.898.696c.441.062.612.636.282.95l-3.522 3.356.83 4.73c.078.443-.36.79-.746.592L8 13.187l-4.389 2.256z" />
